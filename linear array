#include<stdio.h>
void main() {
	int n, key, i, flag = 0, pos = -1; 
	scanf("%d", &n); 
	int arr[n];
	for (i = 0; i < n; i++) { 
		scanf("%d", &arr[i]);
	} 
	scanf("%d", &key);
	for (i = 0; i < n; i++) {
		if (arr[i] == key) { 
			flag = 1;
			pos = i;
			break;
		}
	}
	if (flag == 1) {
		printf("found at position %d\n", pos);
	} else {
		printf("%d not found\n", key);
	}
}
